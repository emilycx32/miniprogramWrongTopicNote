page{
  height:100%;
  background-color: #FCFBFB;
}
.feedback-bg1{
  width: 100%;
  height: 199px;
  position: absolute;
  z-index: 0;
}
.feedback-bg2{
  width: 100%;
  height: 199px;
  position: absolute;
  bottom: 0;
  z-index: 0;
}
.main{
  display: flex;
}
.main-left{
  width: 20px;
  height: 10px;
  float: left;
}
.main-right{
  width: 20px;
  height: 10px;
  float: right;
}
.feedback-text-icon{
  position: absolute;
  width: 65px;
  height: 65px;
  left: 50px;
  top: 18px;
  z-index: 99;
}
.feedback-wrap{
  width: 100%;
  margin-top: 50px;
  background-color: white;
  border-radius: 8px;
  z-index: 1;
  box-shadow:0px 8px 15px 0px rgba(0, 0, 0, 0.05);
}
.feedback-text-wrap{
   height: 186px;
}
.feedback-photos-wrap{
  height: 194px;
}
.feedback-input-wrap{
  display: flex; 
  flex-direction: column;
  align-items: center;
}
.title{
  font-size: 18px;
  color: #464646;
  margin-top: 10px;
  margin-bottom: 10px;
}
.photos-title{
  font-size: 18px;
  color: #464646;
  margin-top: 10px;
}
.subtitle{
  color: #464646;
  font-size: 12px;
}
.text-counter, .photo-counter{
  font-size: 12px;
  color: #616161;
  font-weight: 700;
  letter-spacing: 2px;
  float: right;
}
.feedback-photos-icon{
  position: absolute;
  width: 65px;
  height: 65px;
  left: 50px;
  top: 260px;
  z-index: 99;
}
/* 上传图片样式*/
.ui_uploader{
  width: 90px;
  height: 90px;
  background-color: #EDEDED;
  border-radius: 4px;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 50px;
}
.feedback-photos-input{
  width: 90%;
  margin-top: 18px;
  margin-bottom: 10px;
  display: flex;
  align-items: center;
  justify-content: space-around;
}
.ui_uploader_item{
  display: flex;
  align-items: flex-start;
}
.ui_uploader_item_icon{
  position: absolute;
  margin-top: -8px;
  margin-left: 76px;
}
.submit-button{
  line-height: 96rpx;
  margin: 20px auto 10px auto;
  font-size: 30rpx;
  width: 584rpx;
  height: 96rpx;
  border-radius: 20rpx;
  color: white;
  background-color: #464646;
  display: flex;
  justify-content: center;
  align-items: center;
}
button::after{
  border: none;
}